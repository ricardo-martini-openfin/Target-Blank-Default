{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "17.85.54.32"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Platform app seed local",
        "icon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "name": "Platform app seed local",
        "uninstall-shortcut": true
    },
    "platform": {
        "uuid": "platform_customization_local",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": false,
        "providerUrl": "http://localhost:5555/provider.html",
      
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        
        
        "defaultWindowOptions": {
            "url": "http://localhost:5555/platform-window.html",
            "contextMenu": true,
            "defaultWidth": 600,
            "defaultHeight": 600,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "experimental": {
                "analytics": true
            },
      "ContentCreationRule": {
          "behavior": "window"
      },
        "permissions": {
            "ExternalWindow": {
              	"wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        }
        },
        "defaultViewOptions": {
            "accelerator": {
                "zoom": true
                },
      
            "permissions": {
                "ExternalWindow": {
                      "wrap": true
                },
                "System": {
                    "getAllExternalWindows": true,
                    "launchExternalProcess": true,
                    "readRegistryValue": false,
                    "terminateExternalProcess": true
                }
            }
           
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ]
    },
    "snapshot": {
        "windows": [
            { 
                "layout": {
                    "settings": {
                        "hasHeaders": false
                    }, 
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "https://google.com"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
